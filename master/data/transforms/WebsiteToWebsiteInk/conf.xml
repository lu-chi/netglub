<?xml version='1.0'?>
<!DOCTYPE TransformSchema>
<transform name="WebsiteToWebsiteInk" longName="To Websites [Backlinks]" type="generic" >
  <description>find the websites baclinks</description>
  <parameters>
    <param name="timeout" longName="Time Out" description="the time of the transform" default="15" optional="true" level="advanced" format="int">
      <int min="1"/>
    </param>
  </parameters>
  <config>
    <value name="need_root">true</value>
    <value name="input_as_args">true</value> <!-- otherwise on stdin -->
  </config>
  <input>
    <entity type="website" />
  </input>
  <output>
    <entity type="website" />
  </output>
</transform>
