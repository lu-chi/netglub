<?xml version='1.0'?>
<!DOCTYPE TransformSchema>
<transform name="DomainToLocationWhois" longName="To Location [Whois]" type="generic" >
  <description>get location of an ip</description>
  <parameters>
  </parameters>
  <config>
    <value name="need_root">true</value>
    <value name="input_as_args">true</value> <!-- otherwise on stdin -->
  </config>
  <input>
    <entity type="domain-name" />
  </input>
  <output>
    <entity type="location" />
  </output>
</transform>
