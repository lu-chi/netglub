<?xml version='1.0'?>
<!DOCTYPE TransformSchema>
<transform name="DomainToWebsite" longName="To Website [www.]" type="generic" >
  <description>get a www. website from a domain if it existe</description>
  <parameters>
  </parameters>
  <config>
    <value name="need_root">true</value>
    <value name="input_as_args">true</value> <!-- otherwise on stdin -->
  </config>
  <input>
    <entity type="domain-name" />
  </input>
  <output>
    <entity type="website" />
  </output>
</transform>
