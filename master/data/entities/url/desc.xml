<?xml version='1.0'?>
<!DOCTYPE EntitySchema>
<entity name="url" longName="URL" color="#8a2be2" pngImage="image.png" svgImage="image.svg"><!-- Format color : #RRGGBB where R,G and B are hexa digits -->
  <description>URL</description>
  <fieldList>
    <field name="value" longName="url" description="url" default="" level="default" format="string" ioMode="input">
      <string regex=".*"/>
    </field>
    <field name="title" longName="Page title" description="Page title" default="" optional="true" level="default" format="string" ioMode="output">
      <string regex=".*"/>
    </field>
  </fieldList>
</entity>
