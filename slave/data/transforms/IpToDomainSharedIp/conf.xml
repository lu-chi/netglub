<?xml version='1.0'?>
<!DOCTYPE TransformSchema>
<transform name="IpToDomainSharedIp" longName="To Domain [Shared Ip Address]" type="generic" >
  <description>get the domains that share the same ip as the input ip</description>
  <parameters>
  </parameters>
  <config>
    <value name="need_root">true</value>
    <value name="input_as_args">true</value> <!-- otherwise on stdin -->
  </config>
  <input>
    <entity type="ip-address" />
  </input>
  <output>
    <entity type="domain-name" />
  </output>
</transform>
